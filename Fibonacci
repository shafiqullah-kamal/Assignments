#include <iostream>

using namespace std;

int main()
{
    int a=0,b=1,num,c,count=2;
    cout << "Enter a number" << endl;
    cin >> num;
    cout << " The series is\n" <<endl;
    cout << "1";
    while ( count<=num)
    {
        c=a+b;
        a=b;
        b=c;
        cout <<"\n"<<c;
        count++;
    }
    return 0;
}
